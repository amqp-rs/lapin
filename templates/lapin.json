          }
        ],
        "state": [
          {
            "name": "destination",
            "type": "ShortString",
            "use_str_ref": true
          },
          {
            "name": "source",
            "type": "ShortString",
            "use_str_ref": true
          },
          {
            "name": "routing_key",
            "type": "ShortString",
            "use_str_ref": true
          },
          {
            "name": "creation_arguments",
            "type": "FieldTable"
          }
